{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard-with-typescript",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-unused-vars": ["warn"],
        "no-console": ["warn", {"allow": ["error"]}],
        "@typescript-eslint/quotes": [
            "warn",
            "double"
        ]
    }
}
